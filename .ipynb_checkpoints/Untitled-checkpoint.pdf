   "source": [
    "a = (np.array(np.arange(1,21)*1000).tolist())\n",
    "b = np.array(np.zeros(int(len(a)/2)).tolist() + np.ones(int(len(a)/2)).tolist())\n",
    "\n",
    "plt.scatter(a,b)\n",
    "plt.ylim(-0.05,1.05)\n",
